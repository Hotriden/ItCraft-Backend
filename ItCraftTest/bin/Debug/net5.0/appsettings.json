{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Error"
    },
    "File": {
      "Path": "app.log",
      "Append": "True",
      "FileSizeLimitBytes": 0 // use to activate rolling file behaviour
    }
    },
    "ConnectionStrings": {
      "UserDbConnection": "Server=(LocalDb)\\MSSQLLocalDB; Database=UserDb; Trusted_Connection=True; MultipleActiveResultSets=true"
    },
    "AllowedHosts": "*"
  }
